{
  "layers": {
    "worldpop": {
      "band_sets": [
        {
          "bands": [
            "B1"
          ],
          "dtype": "float32",
          "nodata_vals": [
            -99999
          ]
        }
      ],
      "data_source": {
        "class_path": "rslearn.data_sources.worldpop.WorldPop",
        "init_args": {
          "worldpop_dir": "source_data/worldpop/"
        },
        "query_config": {
          "max_matches": 16
        }
      },
      "type": "raster"
    }
  },
  "tile_store": {
    "class_path": "rslearn.tile_stores.default.DefaultTileStore",
    "init_args": {
      "convert_rasters_to_cogs": false
    }
  }
}
