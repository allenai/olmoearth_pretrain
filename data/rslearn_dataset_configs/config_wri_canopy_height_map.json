{
  "layers": {
    "wri_canopy_height_map": {
      "band_sets": [
        {
          "bands": [
            "B1"
          ],
          "dtype": "uint8",
          "nodata_vals": [
            255
          ]
        }
      ],
      "data_source": {
        "class_path": "rslearn.data_sources.local_files.LocalFiles",
        "init_args": {
          "src_dir": "source_data/meta_chm/"
        },
        "query_config": {
          "max_matches": 16
        }
      },
      "type": "raster"
    }
  },
  "tile_store": {
    "class_path": "rslearn.tile_stores.default.DefaultTileStore",
    "init_args": {
      "convert_rasters_to_cogs": false
    }
  }
}
